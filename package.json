{
  "name": "zoorl-app",
  "version": "0.1.0",
  "bin": {
    "zoorl-app": "bin/zoorl-app.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "test": "jest --maxWorkers=50% --noStackTrace --silent",
    "test:coverage": "jest --maxWorkers=50% --coverage --silent",
    "test:watch": "jest --watch --maxWorkers=25% --noStackTrace --silent",
    "test:integration": "jest --config=src-tests/acceptance/jest.config.js",
    "clear:jest": "jest --clearCache",
    "deploy": "cdk deploy --outputs-file ./cdk-outputs.json --all",
    "deploy:hot": "cdk deploy --outputs-file ./cdk-outputs.json --all --hotswap",
    "cdk:synth": "npm run build && cdk synth",
    "cdk:deploy": "npm run build && cdk deploy --outputs-file ./cdk-outputs.json --all",
    "cdk:destroy": "npm run build && cdk destroy --all",
    "cdk:diff": "npm run build && cdk diff"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.149",
    "@types/jest": "^29.5.14",
    "@types/node": "22.15.19",
    "@types/uuid": "^10.0.0",
    "aws-cdk": "2.1016.0",
    "esbuild": "^0.25.4",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^2.20.0",
    "@aws-lambda-powertools/metrics": "^2.20.0",
    "@aws-lambda-powertools/tracer": "^2.20.0",
    "@aws-sdk/client-cognito-identity": "^3.817.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.821.0",
    "@aws-sdk/client-dynamodb": "^3.817.0",
    "@aws-sdk/credential-provider-cognito-identity": "^3.817.0",
    "@aws-sdk/lib-dynamodb": "^3.817.0",
    "@middy/core": "^6.3.0",
    "@middy/http-json-body-parser": "^5.5.1",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "aws-cdk-lib": "2.196.1",
    "aws-lambda": "^1.0.7",
    "axios": "^1.6.7",
    "constructs": "^10.4.2",
    "middy": "^0.24.0"
  }
}

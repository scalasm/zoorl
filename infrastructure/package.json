{
  "name": "zoorl-infrastructure",
  "version": "0.1.0",
  "bin": {
    "zoorl-infrastructure": "bin/zoorl-infrastructure.js"
  },
  "scripts": {
    "format": "prettier --write .",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "cdk:synth": "npm run build && cdk synth",
    "cdk:deploy": "npm run build && cdk deploy",
    "cdk:destroy": "npm run build && cdk destroy",
    "cdk:diff": "npm run build && cdk diff"
  },
  "devDependencies": {
    "@types/jest": "^29.2.0",
    "@types/node": "18.11.7",
    "@types/prettier": "2.7.1",
    "aws-cdk": "2.49.0",
    "husky": "^8.0.1",
    "jest": "^29.2.2",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "~4.8.4"
  },
  "dependencies": {
    "@aws-cdk/aws-lambda-python-alpha": "^2.49.0-alpha.0",
    "aws-cdk-lib": "2.49.0",
    "constructs": "^10.1.143",
    "source-map-support": "^0.5.21"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
